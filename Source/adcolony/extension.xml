<extension xmlns="http://ns.adobe.com/air/extension/4.0">
    <id>com.tapdaq.airsdk.adcolony</id>
    <versionNumber>1.0</versionNumber>

    <platforms>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>libtapdaqadcolony.jar</nativeLibrary>
                <initializer>com.tapdaq.airsdk.adcolony.Extension</initializer>
                <finalizer>com.tapdaq.airsdk.adcolony.Extension</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libTapdaqAdColony.a</nativeLibrary>
                <initializer>TapdaqAdColonyExtensionInitializer</initializer>
                <finalizer>TapdaqAdColonyExtensionFinalizer</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="default">
            <applicationDeployment />
        </platform>
    </platforms>
</extension>
