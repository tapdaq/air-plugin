<extension xmlns="http://ns.adobe.com/air/extension/4.0">
    <id>com.tapdaq.airsdk.applovin</id>
    <versionNumber>1.0</versionNumber>

    <platforms>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>libtapdaqapplovin.jar</nativeLibrary>
                <initializer>com.tapdaq.airsdk.applovin.Extension</initializer>
                <finalizer>com.tapdaq.airsdk.applovin.Extension</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libTapdaqAppLovin.a</nativeLibrary>
                <initializer>TapdaqAppLovinExtensionInitializer</initializer>
                <finalizer>TapdaqAppLovinExtensionFinalizer</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="default">
            <applicationDeployment />
        </platform>
    </platforms>
</extension>
