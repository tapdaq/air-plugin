<extension xmlns="http://ns.adobe.com/air/extension/4.0">
    <id>com.tapdaq.airsdk.hyprmx</id>
    <versionNumber>1.0</versionNumber>

    <platforms>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>libtapdaqhyprmx.jar</nativeLibrary>
                <initializer>com.tapdaq.airsdk.hyprmx.Extension</initializer>
                <finalizer>com.tapdaq.airsdk.hyprmx.Extension</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libTapdaqHyprMX.a</nativeLibrary>
                <initializer>TapdaqHyprMXExtensionInitializer</initializer>
                <finalizer>TapdaqHyprMXExtensionFinalizer</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="default">
            <applicationDeployment />
        </platform>
    </platforms>
</extension>
