<extension xmlns="http://ns.adobe.com/air/extension/4.0">
    <id>com.tapdaq.airsdk.inmobi</id>
    <versionNumber>1.0</versionNumber>

    <platforms>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>libtapdaqinmobi.jar</nativeLibrary>
                <initializer>com.tapdaq.airsdk.inmobi.Extension</initializer>
                <finalizer>com.tapdaq.airsdk.inmobi.Extension</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libTapdaqInMobi.a</nativeLibrary>
                <initializer>TapdaqInMobiExtensionInitializer</initializer>
                <finalizer>TapdaqInMobiExtensionFinalizer</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="default">
            <applicationDeployment />
        </platform>
    </platforms>
</extension>
