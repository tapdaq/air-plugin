<extension xmlns="http://ns.adobe.com/air/extension/4.0">
  <id>com.tapdaq.airsdk.mopub</id>
  <versionNumber>1.0.0</versionNumber>
  <platforms>

    <platform name="Android-ARM">
        <applicationDeployment>
            <nativeLibrary>libtapdaqmopub.jar</nativeLibrary>
            <initializer>com.tapdaq.airsdk.mopub.Extension</initializer>
            <finalizer>com.tapdaq.airsdk.mopub.Extension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="iPhone-ARM">
        <applicationDeployment>
            <nativeLibrary>libTapdaqMoPub.a</nativeLibrary>
            <initializer>TapdaqMoPubExtensionInitializer</initializer>
            <finalizer>TapdaqMoPubExtensionFinalizer</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="default">
        <applicationDeployment/>
    </platform>

  </platforms>
</extension>
