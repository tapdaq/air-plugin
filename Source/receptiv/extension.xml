<extension xmlns="http://ns.adobe.com/air/extension/4.0">
  <id>com.tapdaq.airsdk.receptiv</id>
  <versionNumber>1.0.0</versionNumber>
  <platforms>

    <platform name="Android-ARM">
        <applicationDeployment>
            <nativeLibrary>libtapdaqreceptiv.jar</nativeLibrary>
            <initializer>com.tapdaq.airsdk.receptiv.Extension</initializer>
            <finalizer>com.tapdaq.airsdk.receptiv.Extension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="iPhone-ARM">
        <applicationDeployment>
            <nativeLibrary>libTapdaqReceptiv.a</nativeLibrary>
            <initializer>TapdaqReceptivExtensionInitializer</initializer>
            <finalizer>TapdaqReceptivExtensionFinalizer</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="default">
        <applicationDeployment/>
    </platform>

  </platforms>
</extension>
