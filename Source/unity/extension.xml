<extension xmlns="http://ns.adobe.com/air/extension/4.0">
    <id>com.tapdaq.airsdk.unity</id>
    <versionNumber>1.0</versionNumber>

    <platforms>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>libtapdaqunity.jar</nativeLibrary>
                <initializer>com.tapdaq.airsdk.unity.Extension</initializer>
                <finalizer>com.tapdaq.airsdk.unity.Extension</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libTapdaqUnityAds.a</nativeLibrary>
                <initializer>TapdaqUnityAdsExtensionInitializer</initializer>
                <finalizer>TapdaqUnityAdsExtensionFinalizer</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="default">
            <applicationDeployment />
        </platform>
    </platforms>
</extension>
